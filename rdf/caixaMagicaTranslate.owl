>
  <rdf:Description rdf:about="http://purl.org/socialparticipation/cm/direction">
    <rdfs:label xml:lang="en">direction</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="pt">direção</rdfs:label>
    <rdfs:comment xml:lang="en">an overall direction of the initiative or some concept important for the group</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/socialparticipation/cm/MagicBox">
    <rdfs:comment xml:lang="en">The magic box of social participations</rdfs:comment>
    <rdfs:label xml:lang="pt">Caixa Mágica</rdfs:label>
    <rdfs:label xml:lang="en">Magic Box</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://purl.org/socialparticipation/cm/LegacyOfProcesses"/>
    <rdfs:comment xml:lang="pt">A caixa mágica de participação social</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/socialparticipation/cm/JointAction">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:range rdf:datatype="http://www.w3.org/2001/XMLSchema#string">4</rdfs:range>
    <rdfs:subClassOf rdf:resource="http://purl.org/socialparticipation/cm/ParticipationLevel"/>
    <rdfs:comment xml:lang="pt">O nível de ação conjunta de participação social.        Este é o quarto nível de participação social.</rdfs:comment>
    <rdfs:label xml:lang="en">Joint Action</rdfs:label>
    <rdfs:label xml:lang="pt">nível</rdfs:label>
    <rdfs:comment xml:lang="en">The joint action level of social participation.        This is the fourth level of social participation</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="pt">Ação Conjunta</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/socialparticipation/cm/FosteringExistingInitiatives">
    <rdfs:label xml:lang="pt">Fomento de Iniciativas Existentes</rdfs:label>
    <rdfs:range rdf:datatype="http://www.w3.org/2001/XMLSchema#string">5</rdfs:range>
    <rdfs:label xml:lang="en">Fostering Existing Initiatives</rdfs:label>
    <rdfs:label xml:lang="pt">nível</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="en">The 'fostering existing initiatives' level of social participation.        This is the fifth level of social participation</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/socialparticipation/cm/ParticipationLevel"/>
    <rdfs:comment xml:lang="pt">O nível de 'fomento de iniciativas existentes' de participação social.        Este é o quinto nível de participação social.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/socialparticipation/cm/Deliberation">
    <rdfs:label xml:lang="pt">nível</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="pt">Deliberação</rdfs:label>
    <rdfs:comment xml:lang="en">The deliberation level of social participation.        This is the third level of social participation</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
