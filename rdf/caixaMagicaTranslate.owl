  <rdf:Description rdf:about="http://purl.org/socialparticipation/cm/participationLevel">
    <rdfs:comment xml:lang="en">o nível de participação de um system ou processo participativo</rdfs:comment>
    <rdfs:comment xml:lang="en">the level of participation (5 levels on our standard conceptualization)</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">participation level</rdfs:label>
    <rdfs:label xml:lang="pt">nível de participação</rdfs:label>
  </rdf:Description>
