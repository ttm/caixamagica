    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">component</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/socialparticipation/cm/ParticipationLevel">
    <rdfs:label xml:lang="en">Participation Level</rdfs:label>
    <rdfs:comment xml:lang="en">The participation level of social participation:         1) Information,         2) Consultation,         3) Deliberation,         4) Joint Action,         5) Fostering Existing Initiatives.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="pt">Nível de Participação</rdfs:label>
    <rdfs:comment xml:lang="pt">O nível de participação social:         1) Informação,         2) Consulta,         3) Deliberação,         4) Ação Conjunta,         5) Fomento de Iniciativas Existentes.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/socialparticipation/cm/beneficiary">
    <rdfs:label xml:lang="en">beneficiary</rdfs:label>
