  <rdf:Description rdf:about="http://purl.org/socialparticipation/cm/Consultation">
    <rdfs:label xml:lang="pt">nível</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://purl.org/socialparticipation/cm/ParticipationLevel"/>
