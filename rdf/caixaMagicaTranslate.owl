  <rdf:Description rdf:about="http://purl.org/socialparticipation/cm/SocialParticipation">
    <rdfs:label xml:lang="pt">Participação Social</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">The inclusion of the civil society in the governance of society</rdfs:comment>
    <rdfs:comment xml:lang="pt">A inclusão da sociedade civil na governança da sociedade</rdfs:comment>
    <rdfs:label xml:lang="en">Social Participation</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://purl.org/socialparticipation/cm/Empowerment"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/socialparticipation/cm/Participabr">
